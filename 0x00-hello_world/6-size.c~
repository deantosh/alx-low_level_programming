#include <stdio.h>

/**
 * main - entry point
 *
 * Return 0 (Success)
 */

int main(void)
{
	/*declare variables*/
	int a;
	long int b;
	long long int c;
	char d;
	float e;

	/*print the size of the data types*/
	printf("Size of a char: byte(s)\n", sizeof(d));
	printf("Size of an int: byte(s)\n", sizeof(a));
	printf("Size of a long int: byte(s)\n", sizeof(b));
	printf("Size of a long long int: byte(s)\n", sizeof(c));
	printf("Size of a float: byte(s)\n", sizeof(d));

	return (0);
}
  
